<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:padding="20dp"
    android:layout_marginTop="10dp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/greenf"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="1dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="@string/pro_no"
                android:background="@color/white"
                android:textSize="18dp"
                />
            <TextView
                android:id="@+id/tv_pro_no_02"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:text="@string/pro_no"
                android:background="@color/white"
                android:textSize="18dp"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="1dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:text="@string/pro_name"
                android:background="@color/white"
                android:textSize="18dp"
                />
            <TextView
                android:id="@+id/tv_pro_name_02"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:text="@string/pro_name"
                android:background="@color/white"
                android:textSize="18dp"
                />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="1dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="@string/base_name"
                android:background="@color/white"
                android:textSize="18dp"
                />
            <TextView
                android:id="@+id/tv_base_name_02"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:text="@string/base_name"
                android:background="@color/white"
                android:textSize="18dp"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="1dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="@string/base_addr"
                android:background="@color/white"
                android:textSize="18dp"
                />
            <TextView
                android:id="@+id/tv_base_addr_02"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:text="@string/base_addr"
                android:background="@color/white"
                android:textSize="18dp"
                />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="1dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="@string/base_area"
                android:background="@color/white"
                android:textSize="18dp"
                />
            <TextView
                android:id="@+id/tv_base_area_02"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:text="@string/base_area"
                android:background="@color/white"
                android:textSize="18dp"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="1dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="@string/pro_num"
                android:background="@color/white"
                android:textSize="18dp"
                />
            <TextView
                android:id="@+id/tv_pro_num_02"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:text="@string/pro_num"
                android:background="@color/white"
                android:textSize="18dp"
                />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="1dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="@string/pro_area"
                android:background="@color/white"
                android:textSize="18dp"
                />
            <TextView
                android:id="@+id/tv_pro_area_02"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:text="@string/pro_area"
                android:background="@color/white"
                android:textSize="18dp"
                />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="1dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="@string/pro_output"
                android:background="@color/white"
                android:textSize="18dp"
                />

            <TextView
                android:id="@+id/tv_pro_output_02"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:background="@color/white"
                android:text="@string/pro_output"
                android:textSize="18dp" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="1dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="@string/pro_ralue"
                android:background="@color/white"
                android:textSize="18dp"
                />
            <TextView
                android:id="@+id/tv_pro_ralue_02"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:text="@string/pro_ralue"
                android:background="@color/white"
                android:textSize="18dp"
                />
        </LinearLayout>




    </LinearLayout>
</LinearLayout>